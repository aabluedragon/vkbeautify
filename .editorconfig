# http://editorconfig.org
root = true

[*.js]
insert_final_newline = ignore
indent_size = 2
indent_style = space
end_of_line = lf

# The JSON files contain newlines inconsistently
[*.json]
insert_final_newline = ignore

# Minified JavaScript files shouldn't be changed
[**.min.js]
indent_style = ignore
insert_final_newline = ignore

[*.md]
trim_trailing_whitespace = false

